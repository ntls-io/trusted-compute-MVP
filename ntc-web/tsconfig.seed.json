{
    "compilerOptions": {
      "target": "ES2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["prisma/**/*", "scripts/**/*"],
    "exclude": ["node_modules"]
  }